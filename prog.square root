Scanner in = new Scanner(System.in);
        double num;
        double raiz = 0;
        System.out.println("Digite a raiz quadrada:");
        num = in.nextDouble();
        raiz = calc(num);
        
        System.out.println("a raiz de " + num + " Ã© " + raiz);
    }
    public static double calc(double num){
        double raiz = 0;
        double aux = 0; 
        
        for(int i = 0; i < num; i++){
            double calc = 1;
            int auxi = 0;
            auxi = (i - 1) * (i - 1);
            if((i * i) > num){
                calc = auxi;
                break;
            }
            raiz = calc;
        }
        
        while((raiz * raiz) != num){
            raiz = (raiz + (num/raiz))/2;
            if(raiz == aux){
                
                break;
            }
            aux = raiz;
        }
        return raiz;
    }

}    
